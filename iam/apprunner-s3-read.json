{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucketArtifacts",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::nikos-fraud-artifacts-2025",
      "Condition": { "StringLike": { "s3:prefix": ["artifacts/*"] } }
    },
    {
      "Sid": "GetModelArtifacts",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::nikos-fraud-artifacts-2025/artifacts/*"
    }
  ]
}
